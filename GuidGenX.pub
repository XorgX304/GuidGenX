StartupAction=DefineVar "[GUIDString]" "String" "" "Global" ""¶DefineVar "[UUIDString]" "String" "" "Global" ""¶DefineVar "[ID]" "String" "" "Global" ""¶DefineVar "[IDHash]" "String" "" "Global" ""¶DefineVar "[HexDigits]" "String" "" "Global" "0123456789abcdef"¶DefineVar "[HexChar]" "String" "" "Global" "0"¶DefineVar "[i]" "Integer" "" "Local" "1"¶DefineVar "[Pos]" "Integer" "" "Local" "0"¶
SubroutineAction=:GenerateV4¶SetVar "[UUIDString]" ""¶.Fill UUID with random Hex¶Loop "1" "32" "[i]"¶  If "[i]" "=" "13"¶    .set mode¶    SetVar "[UUIDString]" "[UUIDString]4"¶  Else¶    If "[i]" "=" "17"¶      .Set the two most significant bits (bits 6 and 7) of the¶      .clock_seq_hi_and_reserved to zero and one, respectively.¶      .this means resulting range is 8...b¶      Random "3" "[Rand]"¶      Math "[Rand]+9" "0" "[Pos]"¶    Else¶      .Full range¶      Random "15" "[Rand]"¶      Math "[Rand]+1" "0" "[Pos]"¶    EndIf¶    SubStr "[HexDigits]" "[Pos]" "1" "[HexChar]"¶    SetVar "[UUIDString]" "[UUIDString][HexChar]"¶  EndIf¶EndLoop¶.AlertBox "Result" "[UUIDString]"¶Return¶¶:GenerateV4bis¶SetVar "[UUIDString]" ""¶.Fill UUID with random Hex¶Loop "1" "16" "[i]"¶  If "[i]" "=" "13"¶    .set mode¶    SetVar "[UUIDString]" "[UUIDString]4"¶  Else¶    .Full range¶    Random "15" "[Rand]"¶    Math "[Rand]+1" "0" "[Pos]"¶    SubStr "[HexDigits]" "[Pos]" "1" "[HexChar]"¶    SetVar "[UUIDString]" "[UUIDString][HexChar]"¶  EndIf¶EndLoop¶¶.hash ID¶Call "VBScript Functions\MD5-hash-string" "[ID]" "[IDHash]"¶.reduce to 16 nibbles¶Call "FoldHash" "[IDHash]" "16" "[IDHash2]"¶.Set the two most significant bits (bits 6 and 7) of the¶.clock_seq_hi_and_reserved to zero and one, respectively.¶SubStr "[IDHash2]" "1" "1" "[Nib]"¶SearchStr "[Nib]" "[HexDigits]" "[Pos]" ""¶While "[Pos]" ">" "4"¶  SetVar "[Pos]" "[Pos]-4"¶EndWhile¶SetVar "[Pos]" "[Pos]+8"¶SubStr "[HexDigits]" "[Pos]" "1" "[HexChar]"¶.replace nibble¶StrDel "[IDHash2]" "1" "1" "[IDHash2]"¶StrIns "[HexChar]" "[IDHash2]" "1" "[IDHash2]"¶.complete string 16 nibbles random + 16 nibbles IDHash¶SetVar "[UUIDString]" "[UUIDString][IDHash2]"¶Return¶¶:GUIDFormat¶SubStr "[UUIDString]" "1" "8" "[Segment1]"¶SubStr "[UUIDString]" "9" "4" "[Segment2]"¶SubStr "[UUIDString]" "13" "4" "[Segment3]"¶SubStr "[UUIDString]" "17" "4" "[Segment4]"¶SubStr "[UUIDString]" "21" "12" "[Segment5]"¶SetVar "[GUIDString]" "{[Segment1]-"¶SetVar "[GUIDString]" "[GUIDString][Segment2]-"¶SetVar "[GUIDString]" "[GUIDString][Segment3]-"¶SetVar "[GUIDString]" "[GUIDString][Segment4]-"¶SetVar "[GUIDString]" "[GUIDString][Segment5]}"¶SetVar "[IDLString]" "[Segment1]-[Segment2]-[Segment3]-[Segment4]-[Segment5]"¶Return¶
